# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=document_creation
POSTGRES_PORT=5432

# Database URL (used by Drizzle)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/document_creation

# Keycloak
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KEYCLOAK_URL=http://localhost:8080
KEYCLOAK_REALM=document-creation
KEYCLOAK_CLIENT_ID=sveltekit-app
KEYCLOAK_CLIENT_SECRET=your-client-secret-here

# Keycloak Admin API (for managing API clients)
# Option 1: Use admin credentials (simpler for development)
# Option 2: Create a service account with realm-management roles
KEYCLOAK_ADMIN_API_CLIENT_ID=admin-cli
KEYCLOAK_ADMIN_API_CLIENT_SECRET=

# Application
ORIGIN=http://localhost:5173
